    "# Linear search implementation\n",
    "arr = [1, 2, 3, 4, 5]\n",
    "\n",
    "def search(target):\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i]==target:\n",
    "            return i\n",
    "        \n",
    "    return \"Not found\"\n",
    "print(search(6))  \n"
